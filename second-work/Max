import java.util.Scanner;

public class Max {

	public int getMax(int a,int b){
		return a>b?a:b;
	}
	
	public static void main(String[] args) {
		// TODO Auto-generated method stub
		Max m=new Max();
		int a,b;
		Scanner sc=new Scanner(System.in);
		a=sc.nextInt();
		b=sc.nextInt();
		System.out.println(m.getMax(a,b));
	}

}
